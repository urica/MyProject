	public AbsoluteLayout LogInPage(){
		final AbsoluteLayout layout = new AbsoluteLayout();
		setContent(layout);
		layout.setWidth("100.0%");
		layout.setHeight("100.0%");
		layout.setStyleName("backColor");
	
		TextField text = new TextField();
		text.setWidth("200px");
		text.setHeight("-1px");
		layout.addComponent(text, "top: 408.0px; left: 600.0px");
		
		PasswordField p = new PasswordField();
		p.setHeight("-1px");
		p.setWidth("200px");
		layout.addComponent(p, "top: 470.0px; left: 600.0px");
	
		
		Label username = new Label("Username");
		username.setWidth("80px");
		username.setHeight("25px");
		layout.addComponent(username, "top: 414.0px; left: 460.0px");
		
		Label password_label = new Label("Password");
		password_label.setWidth("80px");
		password_label.setHeight("25px");
		layout.addComponent(password_label,"top: 474.0px; left: 460.0px");
		
		Button button = new Button( "Submit");
		button.setWidth("-1px");
		button.setHeight("-1px");
		layout.addComponent(button, "top: 548.0px; left: 713.0px;");

		
		/*String basepath = VaadinService.getCurrent()
                .getBaseDirectory().getAbsolutePath();

		// Image as a file resource
		FileResource resource = new FileResource(new File(basepath +
		                      "/WEB-INF/images/images.jpg"));
		
		// Show the image in the application
		Image image = new Image("Image from file", resource);
		image.setWidth("537px");
		image.setHeight("137px");
		image.setVisible(true);
		layout.addComponent(image, "top: 0.0px; left: 382.0px");*/
		
		layout.setStyleName("log");
		
		button.addClickListener(new Button.ClickListener() {
			
			@Override
			public void buttonClick(ClickEvent event) {
				String uid = text.getValue();
				String pass = p.getValue();
				ctr = new UserCtrl(uid,ur);
				value = ctr.logUser(uid,pass);
				
				if(value == 1){
					studentPage();
				}
				if(value==2){
					profesorPage();
				}
				if(value==3){
					adminPage();
				}
				if(value==4){
					chifPage();
				}
				/*if(text.getValue().equals("student")){
					studentPage();
					value=1;
				}
				else if(text.getValue().equals("profesor")){
					profesorPage();
					value=2;
				}
				else if(text.getValue().equals("admin")){
					adminPage();
					value=3;
				}
				else if(text.getValue().equals("cif")){
					chifPage();
					value=4;
				}
				else
					layout.addComponent(new Label(text.getValue()));*/
				
			}

			
		});
		
		
		return layout;
	}
	
	
