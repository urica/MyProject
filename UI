	public AbsoluteLayout studentPage(){
		
		AbsoluteLayout layout2 = new AbsoluteLayout();
		setContent(layout2);
		layout2.setWidth("100.0%");
		layout2.setHeight("100.0%");
		layout2.setStyleName("v-switch-slider-gruppe");
		
		layout2.addComponent(panel(),"top:0px;left:0px");
		layout2.addComponent(dropmenu(),"top:15px;right:50px");
		layout2.addComponent(name(),"top:25px;right:60px");
		
		Button b1 = new Button("See Grades");
		b1.setStyleName("border");
		b1.setWidth("420px");
		b1.setHeight("102px");
		layout2.addComponent(b1,"top: 220.0px; left: 500.0px;");
		
		
		
		Button b2 = new Button("Study Contract");
		b2.setWidth("420px");
		b2.setHeight("102px");
		b2.setStyleName("border");
		layout2.addComponent(b2, "top: 340.0px; left: 500.0px;" );
		
		
		
		Button b3 = new Button("Taxes");
		b3.setWidth("420px");
		b3.setHeight("102px");
		b3.setStyleName("border");
		layout2.addComponent(b3,"top: 460.0px; left: 500.0px;");
		
		b1.addClickListener(new Button.ClickListener() {
			
			@Override
			public void buttonClick(ClickEvent event) {
				
				studentGrades();
			}
		});
		
		b2.addClickListener(new Button.ClickListener() {
			
			@Override
			public void buttonClick(ClickEvent event) {
				
				studentContract();
			}
		});
		
		b3.addClickListener(new Button.ClickListener() {
			
			@Override
			public void buttonClick(ClickEvent event) {
				
				studentRestante();
			}
		});
		
		return layout2;
	}
	
	
