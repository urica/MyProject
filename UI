		layout.addComponent(panel(),"top:0px;left:0px");
		layout.addComponent(dropmenu(),"top:15px;right:50px");
		layout.addComponent(name(),"top:25px;right:60px");
		Button b = new Button("Back");
		
		
		b.addClickListener(new Button.ClickListener() {
			
			@Override
			public void buttonClick(ClickEvent event) {
				if(value==2){
				profesorPage();}
				if(value==4){
					chifPage();
				}
			}
		});
		layout.addComponent(b,"top:20px;left:35px");
		
		
		
		NativeSelect check = new NativeSelect("Choose Cours");
		check.setStyleName("black");
		check.setWidth("200px");
		check.setHeight("40px");
		check.addItem("a");
		check.addItem("b");
		check.addItem("c");
		
		layout.addComponent(check,"top: 125.0px; left: 100.0px;");
		
		
		Grid grid = new Grid();
		grid.setEditorEnabled(true);
		grid.addColumn("Name", String.class);
		grid.addColumn("ID", Integer.class);
		grid.addColumn("Group",Integer.class);
		grid.addColumn("Semester",Integer.class);
		grid.addColumn("Grade",Integer.class);
		grid.addColumn("Promovate",String.class);

/*		// Add some data rows
		grid.addRow("Nicolaus Copernicus", 1543);
		grid.addRow("Galileo Galilei", 1564);
		grid.addRow("Johannes Kepler", 1571);*/
		grid.addRow("Sam",1,445,3,0,"false");
		
		grid.setColumnOrder("Name", "ID");
		layout.addComponent(grid,"top:125px;left:400px");
		
		return layout;
	}
	
	
